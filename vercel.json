{
  "version": 2,
  "routes": [
    {
      "src": "/api/register",
      "dest": "/api/register.js"
    },
    {
      "src": "/api/participants",
      "dest": "/api/participants.js"
    },
    {
      "src": "/api/stats",
      "dest": "/api/stats.js"
    }
  ],
  "env": {
    "KV_REST_API_URL": "@kv_rest_api_url",
    "KV_REST_API_TOKEN": "@kv_rest_api_token"
  },
  "functions": {
    "api/**/*.js": {
      "maxDuration": 10
    }
  }
}
